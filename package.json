    "Nprogress",
    "React nprogress",
    "top loading indicator"
  ],
  "author": "Mahmudul Alam <expelmahmud@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/devmahmud/next-progress-bar/issues"
  },
  "homepage": "https://github.com/devmahmud/next-progress-bar#readme",
  "files": [
    "dist/**/*.js*",
    "dist/**/*.mjs*",
    "dist/**/*.d*"
  ],
  "peerDependencies": {
    "react": ">= 16.0.1",
    "react-dom": ">= 16.0.1"
  },
  "devDependencies": {
    "@types/node": "^20.9.1",
    "@types/react": "^18.2.37",
    "@typescript-eslint/eslint-plugin": "^6.11.0",
    "@typescript-eslint/parser": "^6.11.0",
    "eslint": "^8.53.0",
    "prettier": "^3.1.0",
    "react": "^18.2.0",
    "tsup": "^7.3.0",
    "typescript": "^5.2.2"
  }
