{
  "name": "next-transition-bar",
  "version": "1.0.0",
  "description": "Progress-bar component for Next.js app.",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "tsup",
    "prepack": "npm run build",
    "lint": "eslint --fix --cache src/",
    "format": "prettier --write \"src/**/*.{ts,tsx}\"",
    "typecheck": "tsc -p tsconfig.json"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/devmahmud/next-progress-bar.git"
  },
  "keywords": [
    "Nprogress",
    "Next",
    "Next.js",
    "Next.js 13",
    "Next.js 14",
    "app router",
